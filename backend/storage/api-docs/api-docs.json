        "/v1/register/pasien": {
            "post": {
                "tags": [
                    "User"
                ],
                "summary": "Register a new pasien",
                "operationId": "1d0c0ab5739307e0b25ebbd038c11254",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "tanggal_lahir",
                                    "gender",
                                    "no_telp"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "John Doe"
                                    },
                                    "tanggal_lahir": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "2012-12-31"
                                    },
                                    "gender": {
                                        "description": "Jenis kelamin, hanya diisi 'laki-laki' atau 'perempuan'",
                                        "type": "string",
                                        "example": "laki-laki"
                                    },
                                    "no_telp": {
                                        "description": "Nomor telepon, hanya angka, 10 sampai 15 digit",
                                        "type": "string",
                                        "example": "081234567890"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User registered successfully"
                    },
                    "400": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "The given data was invalid."
                                        },
                                        "errors": {
                                            "properties": {
                                                "name": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string",
                                                        "example": "Nama sudah terdaftar, silakan gunakan nama lain."
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
