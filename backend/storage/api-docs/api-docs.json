        "/v1/videos": {
            "get": {
                "tags": [
                    "Videos"
                ],
                "summary": "Get all videos",
                "operationId": "2dd39694f233ae275064d4e27e73aea0",
                "responses": {
                    "200": {
                        "description": "List of all videos",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "properties": {
                                            "id": {
                                                "type": "integer"
                                            },
                                            "raw_video_url": {
                                                "type": "string"
                                            },
                                            "processed_video_url": {
                                                "type": "string"
                                            },
                                            "status": {
                                                "type": "string"
                                            },
                                            "user": {
                                                "properties": {
                                                    "id": {
                                                        "type": "integer"
                                                    },
                                                    "name": {
                                                        "type": "string"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        },
                                        "type": "object"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Videos"
                ],
                "summary": "Upload a pair of videos (raw and processed)",
                "operationId": "766f06dc2935b23771613a91839bb1a1",
                "requestBody": {
                    "description": "Upload raw and processed videos",
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "raw_video",
                                    "processed_video"
                                ],
                                "properties": {
                                    "raw_video": {
                                        "description": "Raw video file",
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "processed_video": {
                                        "description": "Processed video file with bounding box",
                                        "type": "string",
                                        "format": "binary"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Videos uploaded successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string"
                                        },
                                        "data": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer"
                                                },
                                                "raw_video_path": {
                                                    "type": "string"
                                                },
                                                "processed_video_path": {
                                                    "type": "string"
                                                },
                                                "status": {
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation error"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
