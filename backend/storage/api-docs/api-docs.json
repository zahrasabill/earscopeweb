        "/v1/videos/stream/{filename}": {
            "get": {
                "tags": [
                    "Videos"
                ],
                "summary": "Stream a video file",
                "operationId": "1b927ddc0005c5ebec55da753647ef4a",
                "parameters": [
                    {
                        "name": "filename",
                        "in": "path",
